{
    "manifest_version": 2,
    "name": "路径转二维码",
    "version": "1.0",
    "description": "网页的路径转二维码，提供手机下载",
	"permissions": ["tabs"],
    "icons": {
        "16": "images/logo.png",
        "48": "images/logo.png",
        "128": "images/logo.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "images/logo.png",
            "38": "images/logo.png"
        },
        "default_title": "手机查看地址",
        "default_popup": "popup.html"
    }
}